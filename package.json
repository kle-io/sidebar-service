{
  "name": "sidebar-service",
  "description": "> a clone of a Soundcloud song page built with service oriented architecture",
  "author": "Desmond Rogers",
  "license": "ISC",
  "engines": {
    "node": ">=6.13.0"
  },
  "scripts": {
    "start": "node -r dotenv/config server/index.js",
    "dev": "webpack --mode development -d --watch",
    "build": "webpack -p",
    "deploy": "grunt aws_s3",
    "db:setup": "npx sequelize db:create && npx sequelize db:migrate",
    "postdb:setup": "npx sequelize db:seed:all",
    "lint": "npx eslint client/src/*",
    "test": "npx jest",
    "test:coverage": "npm test -- --coverage"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "mysql2": "^2.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-transition-group": "^4.3.0",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-transform-modules-commonjs": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-preset-airbnb": "^4.4.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-hackreactor": "git://github.com/reactorcore/eslint-config-hackreactor.git",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "grunt": "^1.5.3",
    "grunt-aws-s3": "^2.0.1",
    "jest": "^24.9.0",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "version": "1.0.0",
  "main": ".eslintrc.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kle-io/sidebar-service.git"
  },
  "bugs": {
    "url": "https://github.com/kle-io/sidebar-service/issues"
  },
  "homepage": "https://github.com/kle-io/sidebar-service#readme"
}
